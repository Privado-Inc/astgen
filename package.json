{
  "name": "@joernio/astgen",
  "version": "2.0.4",
  "description": "Generate AST in json format for a number of languages by invoking appropriate tool",
  "exports": "./index.js",
  "keywords": [
    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./bin/astgen.js"
  },
  "repository": "https://github.com/joernio/astgen",
  "author": "Team joernio",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.17.9",
    "svelte": "^3.47.0",
    "yargs": "^17.4.1"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "postinstall": "pkg . --no-bytecode --no-native-build --public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "*.js",
    "bin/"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "pkg": "^5.8.0"
  }
}
