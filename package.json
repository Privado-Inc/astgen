{
  "name": "@joernio/astgen",
  "version": "0.1.0",
  "description": "Generate JS/TS AST in json format with Babel",
  "exports": "./index.js",
  "keywords": [
    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./bin/astgen.js"
  },
  "repository": "https://github.com/max-leuthaeuser/astgen",
  "author": "Max Leuthaeuser",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.17.9",
    "typescript": "^4.9.4",
    "yargs": "^17.4.1"
  },
  "scripts": {
    "postinstall": "pkg . --no-bytecode --no-native-build --public --compress GZip"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "*.js",
    "bin/"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3",
    "pkg": "^5.8.0"
  }
}
