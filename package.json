    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./bin/astgen.js"
  },
  "type": "module",
  "repository": "https://github.com/joernio/astgen",
  "author": "Team joernio",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.14.1",
    "dockerfile-ast": "^0.2.1",
    "mdast-util-from-markdown": "^0.8.5",
